{
	"webroot": "wwwroot",
	"version": "1.0.0-*",

	"dependencies": {
		"Microsoft.AspNet.Diagnostics": "1.0.0-beta7",
		"Microsoft.AspNet.Mvc": "6.0.0-beta7",
		"Microsoft.AspNet.Mvc.TagHelpers": "6.0.0-beta7",
		"Microsoft.AspNet.Routing": "1.0.0-beta7",
		"Microsoft.AspNet.Server.IIS": "1.0.0-beta7",
		"Microsoft.AspNet.Server.WebListener": "1.0.0-beta7",
		"Microsoft.AspNet.StaticFiles": "1.0.0-beta7",
		"Microsoft.AspNet.Tooling.Razor": "1.0.0-beta7",
		"Microsoft.Framework.Configuration.Abstractions": "1.0.0-beta7",
		"Microsoft.Framework.Configuration.Json": "1.0.0-beta7",
		"Microsoft.Framework.Logging": "1.0.0-beta7",
		"Microsoft.Framework.Logging.Console": "1.0.0-beta7",
		"Microsoft.VisualStudio.Web.BrowserLink.Loader": "14.0.0-beta7",

		"SaasKit.Multitenancy.AspNet5": "0.0.2-pre"
	},

	"commands": {
		"web": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --server.urls=http://localhost:5000;http://localhost:5001;http://localhost:5009"
	},

	"frameworks": {
		"dnx451": { },
		"dnxcore50": { }
	},

	"publishExclude": [
		"node_modules",
		"bower_components",
		"**.xproj",
		"**.user",
		"**.vspscc"
	],
	"exclude": [
		"wwwroot",
		"node_modules",
		"bower_components"
	]
}
